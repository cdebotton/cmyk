{
  "name": "@cmyk/apps",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "apollo:build": "apollo codegen:generate --addTypename --target typescript --outputFlat ./src/types.ts",
    "apollo:watch": "yarn apollo:build --watch --queries '**/*.{tsx,ts}'",
    "cra:start": "react-scripts start",
    "cra:build": "react-scripts build",
    "build": "npm-run-all 'apollo:build' 'cra:build'",
    "start": "npm-run-all --parallel 'cra:start' 'apollo:watch'",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-regular-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link": "^1.2.6",
    "apollo-link-context": "^1.0.12",
    "apollo-link-error": "^1.1.5",
    "apollo-link-http": "^1.5.9",
    "apollo-link-ws": "^1.0.12",
    "apollo-upload-client": "10.0.0",
    "date-fns": "^1.30.1",
    "graphql": "14.0.2",
    "graphql-tag": "^2.10.0",
    "polished": "^2.3.1",
    "react": "16.7.0-alpha.2",
    "react-apollo": "^2.3.3",
    "react-apollo-hooks": "0.2.1",
    "react-cache": "2.0.0-alpha.1",
    "react-dom": "16.7.0-alpha.2",
    "react-router-dom": "4.4.0-beta.6",
    "react-scripts": "2.1.3",
    "react-spring": "7.2.7",
    "styled-components": "^4.1.3",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^8.1.1",
    "@types/react": "16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "4.1.4",
    "@types/yup": "^0.26.3",
    "jest": "^23.6.0",
    "jest-dom": "^3.0.0",
    "jest-watch-typeahead": "^0.2.0",
    "serve": "^10.1.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
