  "scripts": {
    "apollo-codegen": "apollo-codegen generate src/**/*.tsx --project-name app --tag-name gql --target typescript --output src/app/generated/operation-result-types.ts",
    "prebuild:babel": "mkdir -p dist/http",
    "build:babel": "babel --out-dir dist/http --extensions .ts src/http",
    "prebuild": "rm -rf dist/* && stylelint src/**/*.tsx && eslint src src/**/*.ts src/**/*.tsx && tsc --noEmit",
    "build": "npm-run-all 'build:babel' 'build:webpack'",
    "watch:babel": "yarn build:babel --watch",
    "build:webpack": "webpack",
    "watch:webpack": "webpack-serve",
    "prewatch": "rm -rf dist",
    "watch": "NODE_ENV=development npm-run-all --parallel 'watch:webpack' 'nodemon'",
    "run": "node dist/http/index.js",
    "nodemon": "nodemon src/http/index.ts",
    "start": "NODE_ENV=production npm-run-all 'build' 'run'"
  },
