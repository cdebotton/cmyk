{
  "name": "cmyk",
  "version": "1.0.0",
  "main": "dist/http/index.js",
  "author": "Christian de Botton <cdebotton@gmail.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild:babel": "mkdir -p dist/server",
    "build:babel": "babel --out-dir dist/server --extensions .ts,.tsx src/",
    "prebuild": "rm -rf dist/*",
    "build": "npm-run-all 'build:babel' 'build:webpack'",
    "watch:babel": "yarn build:babel --watch",
    "build:webpack": "webpack",
    "watch:webpack": "webpack-serve",
    "prewatch": "rm -rf dist",
    "watch": "NODE_ENV=development npm-run-all --parallel 'watch:webpack' 'watch:postgraphile' 'nodemon'",
    "run": "node dist/server/http/index.js",
    "postgraphile": "postgraphile --schema cmyk -c \"postgres://localhost:5432\"",
    "watch:postgraphile": "yarn postgraphile --watch",
    "nodemon": "wait-on dist/react-loadable.json && nodemon src/http/index.tsx",
    "start": "NODE_ENV=production npm-run-all 'build' 'run'"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.2",
    "apollo-client": "2.3.2",
    "apollo-link-http": "1.5.4",
    "boom": "^7.2.0",
    "dotenv": "^6.0.0",
    "graphql": "0.13.2",
    "koa": "2.5.1",
    "koa-compress": "3.0.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-static": "4.0.3",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.3",
    "postgraphile": "4.0.0-beta.10",
    "react": "16.4.0",
    "react-apollo": "2.1.4",
    "react-dom": "16.4.0",
    "react-hot-loader": "4.2.0",
    "react-loadable": "^5.4.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.49",
    "@babel/node": "7.0.0-beta.49",
    "@babel/polyfill": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@babel/preset-react": "7.0.0-beta.49",
    "@babel/preset-stage-0": "7.0.0-beta.49",
    "@babel/preset-typescript": "7.0.0-beta.49",
    "@types/boom": "^7.2.0",
    "@types/koa": "2.0.46",
    "@types/koa-bodyparser": "4.2.0",
    "@types/koa-compress": "2.0.8",
    "@types/koa-mount": "3.0.1",
    "@types/koa-router": "7.0.28",
    "@types/koa-static": "4.0.0",
    "@types/node-fetch": "^2.1.1",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.5",
    "@types/react-loadable": "^5.4.0",
    "@types/react-router": "4.0.26",
    "@types/react-router-dom": "4.2.7",
    "babel-loader": "8.0.0-beta.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "polished": "1.9.2",
    "styled-components": "3.3.0",
    "typescript": "2.9.1",
    "wait-on": "^2.1.0",
    "webpack": "4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-serve": "1.0.2"
  }
}
